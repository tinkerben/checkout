{
  "name": "sk2",
  "version": "1.0.0",
  "description": "Code Challenge",
  "author": "KB",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinkerben/ad-pricing-system.git"
  },
  "scripts": {
    "test": "LOG_LEVEL=ERROR jest --testRegex=\\\\.spec\\\\.tsx?$ --detectOpenHandles",
    "build": "tsc",
    "start": "node build/src/index.js",
    "docker:up": "docker build -t checkout . && docker run -it checkout /bin/bash"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "undefined": "^0.1.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.164",
    "lodash": "^4.17.20",
    "tslib": "^2.0.3"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-results"
        }
      ]
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(test|spec|ispec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true
  }
}
